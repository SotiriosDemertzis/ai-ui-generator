  "borderRadius": {
    "none": "0px",
    "xs": "2px",
    "sm": "4px",
    "md": "6px",
    "lg": "8px",
    "xl": "12px",
    "2xl": "16px",
    "3xl": "24px",
    "full": "9999px"
  },
  "shadows": {
    "xs": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
    "sm": "0 1px 3px 0 rgb(0 0 0 / 0.1), 0 1px 2px -1px rgb(0 0 0 / 0.1)",
    "md": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
    "lg": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
    "xl": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)",
    "2xl": "0 25px 50px -12px rgb(0 0 0 / 0.25)",
    "inner": "inset 0 2px 4px 0 rgb(0 0 0 / 0.05)",
    "colored": {
      "primary": "0 4px 14px 0 rgb(59 130 246 / 0.15)",
      "secondary": "0 4px 14px 0 rgb(139 92 246 / 0.15)",
      "success": "0 4px 14px 0 rgb(16 185 129 / 0.15)",
      "error": "0 4px 14px 0 rgb(239 68 68 / 0.15)"
    }
  },
  "animations": {
    "durations": {
      "fast": "150ms",
      "normal": "200ms",
      "slow": "300ms",
      "slower": "500ms"
    },
    "easings": {
      "linear": "linear",
      "in": "cubic-bezier(0.4, 0, 1, 1)",
      "out": "cubic-bezier(0, 0, 0.2, 1)",
      "inOut": "cubic-bezier(0.4, 0, 0.2, 1)",
      "spring": "cubic-bezier(0.175, 0.885, 0.32, 1.275)"
    }
  },
  "effects": {
    "backdrop": {
      "blur": "backdrop-blur-sm",
      "blurMd": "backdrop-blur-md",
      "blurLg": "backdrop-blur-lg"
    },
    "glass": {
      "light": "bg-white/20 backdrop-blur-md border border-white/30",
      "dark": "bg-gray-800/20 backdrop-blur-md border border-gray-700/30"
    },
    "gradients": {
      "primary": "bg-gradient-to-r from-blue-600 to-blue-700",
      "secondary": "bg-gradient-to-r from-purple-600 to-purple-700",
      "success": "bg-gradient-to-r from-green-600 to-green-700",
      "warm": "bg-gradient-to-r from-orange-400 via-red-500 to-pink-600",
      "cool": "bg-gradient-to-r from-cyan-400 via-blue-500 to-purple-600",
      "subtle": "bg-gradient-to-r from-gray-50 to-gray-100"
    }
  },
  "modernUIPatterns": {
    "glassmorphism": {
      "enabled": true,
      "styles": {
        "primary": {
          "background": "rgba(255, 255, 255, 0.25)",
          "backdropFilter": "blur(16px)",
          "border": "1px solid rgba(255, 255, 255, 0.18)",
          "boxShadow": "0 8px 32px 0 rgba(31, 38, 135, 0.37)"
        },
        "secondary": {
          "background": "rgba(255, 255, 255, 0.1)", 
          "backdropFilter": "blur(10px)",
          "border": "1px solid rgba(255, 255, 255, 0.12)",
          "boxShadow": "0 4px 16px 0 rgba(31, 38, 135, 0.2)"
        },
        "dark": {
          "background": "rgba(17, 25, 40, 0.3)",
          "backdropFilter": "blur(16px)",
          "border": "1px solid rgba(255, 255, 255, 0.125)",
          "boxShadow": "0 8px 32px 0 rgba(0, 0, 0, 0.3)"
        }
      }
    },
    "neumorphism": {
      "enabled": true,
      "styles": {
        "light": {
          "background": "#ecf0f3",
          "borderRadius": "20px",
          "boxShadow": "20px 20px 60px #bebebe, -20px -20px 60px #ffffff"
        },
        "dark": {
          "background": "#2d3748",
          "borderRadius": "20px", 
          "boxShadow": "20px 20px 60px #1a202c, -20px -20px 60px #404c64"
        },
        "inset": {
          "boxShadow": "inset 20px 20px 60px #bebebe, inset -20px -20px 60px #ffffff"
        }
      }
    },
    "microInteractions": {
      "buttonHover": {
        "transform": "translateY(-2px)",
        "boxShadow": "0 10px 25px rgba(0,0,0,0.15)",
        "transition": "all 0.3s cubic-bezier(0.4, 0, 0.2, 1)",
        "scale": "1.05"
      },
      "cardHover": {
        "transform": "translateY(-8px) scale(1.02)",
        "boxShadow": "0 25px 50px rgba(0,0,0,0.15)",
        "transition": "all 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      },
      "inputFocus": {
        "borderColor": "#4f46e5",
        "boxShadow": "0 0 0 3px rgba(79, 70, 229, 0.1)",
        "transition": "all 0.2s ease",
        "transform": "scale(1.02)"
      },
      "ripple": {
        "position": "relative",
        "overflow": "hidden",
        "transform": "scale(1)",
        "transition": "transform 0.2s"
      }
    },
    "animations": {
      "fadeInUp": {
        "keyframes": "@keyframes fadeInUp { from { opacity: 0; transform: translateY(30px); } to { opacity: 1; transform: translateY(0); } }",
        "animation": "fadeInUp 0.6s cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      },
      "slideInRight": {
        "keyframes": "@keyframes slideInRight { from { opacity: 0; transform: translateX(30px); } to { opacity: 1; transform: translateX(0); } }",
        "animation": "slideInRight 0.5s cubic-bezier(0.4, 0, 0.2, 1) 0.1s both"
      },
      "scaleIn": {
        "keyframes": "@keyframes scaleIn { from { opacity: 0; transform: scale(0.9); } to { opacity: 1; transform: scale(1); } }",
        "animation": "scaleIn 0.3s cubic-bezier(0.68, -0.55, 0.265, 1.55)"
      },
      "floatIn": {
        "keyframes": "@keyframes floatIn { 0% { opacity: 0; transform: translateY(20px) scale(0.95); } 100% { opacity: 1; transform: translateY(0) scale(1); } }",
        "animation": "floatIn 0.8s cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      }
    },
    "modernGradients": {
      "sunset": {
        "background": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)"
      },
      "ocean": {
        "background": "linear-gradient(135deg, #667db6 0%, #0082c8 100%)"
      },
      "forest": {
        "background": "linear-gradient(135deg, #134e5e 0%, #71b280 100%)"
      },
      "cosmic": {
        "background": "linear-gradient(135deg, #8360c3 0%, #2ebf91 100%)"
      },
      "aurora": {
        "background": "linear-gradient(135deg, #a8edea 0%, #fed6e3 100%)"
      },
      "neon": {
        "background": "linear-gradient(135deg, #12c2e9 0%, #c471ed 50%, #f64f59 100%)"
      }
    }
  },
  "breakpoints": {
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  }
